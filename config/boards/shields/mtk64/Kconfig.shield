config SHIELD_MTK64_R
    def_bool $(shields_list_contains,mtk64_R)

config SHIELD_MTK64_L
    def_bool $(shields_list_contains,mtk64_L)

config SHIELD_MTK64_DONGLE
    def_bool $(shields_list_contains,mtk64_DONGLE)

config MTK64_DONGLE_STATUS_LED
    bool "Enable status LED support for mtk64 dongle"
    default n
    depends on SHIELD_MTK64_DONGLE